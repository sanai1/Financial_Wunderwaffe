    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.budget.fragments.BudgetHistoryFragment">

    <include layout="@layout/main_color"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
